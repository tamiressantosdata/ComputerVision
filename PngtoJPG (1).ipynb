        "import cv2\n",
        "import sys\n",
        "\n",
        "image = cv2.imread('lena.png')\n",
        "if image is None:\n",
        "    print('Failed to read image from file')\n",
        "    sys.exit(1)\n",
        "\n",
        "success = cv2.imwrite('MyPic.jpg', image)\n",
        "if not success:\n",
        "    print('Failed to write image to file')\n",
        "    sys.exit(1)"
      ]
    }
  ]
